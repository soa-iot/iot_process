    <userTask id="repair" name="维修分配" activiti:candidateUsers="repairor"></userTask>
    <sequenceFlow id="mechatronicsFlow31" sourceRef="pure" targetRef="repair"></sequenceFlow>
    <userTask id="arrange" name="作业安排" activiti:assignee="arrangor"></userTask>
    <userTask id="receive" name="作业接收" activiti:candidateUsers="receivor"></userTask>
    <userTask id="check" name="作业验收" activiti:candidateUsers="checkor"></userTask>
