    <endEvent id="endprocess" name="闭环流程"></endEvent>
    <sequenceFlow id="mechatronicsFlow8" sourceRef="check" targetRef="endprocess"></sequenceFlow>
    <userTask id="report" name="问题上报" activiti:assignee="reporter">
      <extensionElements>
        <activiti:executionListener event="end" class="cn.soa.service.impl.activity.FindEstimatorByArea"></activiti:executionListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startprocess" targetRef="report"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="report" targetRef="estimate"></sequenceFlow>
    <exclusiveGateway id="node1" name="分支节点1"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="estimate" targetRef="node1"></sequenceFlow>
    <sequenceFlow id="flow12" name="分支1流向2" sourceRef="node1" targetRef="pure">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[area == {'净化工段'} and isIngroup={3}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="分支1流向1" sourceRef="node1" targetRef="repair">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[area == {'维修工段'}  and  isIngroup={3}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="repair" targetRef="arrange"></sequenceFlow>
    <userTask id="complement" name="作业完成" activiti:candidateUsers="complementor"></userTask>
    <sequenceFlow id="flow7" sourceRef="receive" targetRef="complement"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="complement" targetRef="check"></sequenceFlow>
    <sequenceFlow id="flow13" name="分支1流向3" sourceRef="node1" targetRef="receive">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[isIngroup={1}]]></conditionExpression>
    </sequenceFlow>
